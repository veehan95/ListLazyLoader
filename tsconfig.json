{
  "compilerOptions": {
    "resolveJsonModule": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "Node",
    "lib": [ "ES2015", "dom" ],
    "outDir": "./lazyLoader",
    "strict": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "removeComments": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "**/type.ts"
  ]
}
